	<ItemGroup>
	  <Content Include="Resources\fonts\undefeated.ttf">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

	<ItemGroup>
	  <EmbeddedResource Include="Resources\ClickSounds\Creamy.wav">
	    <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
