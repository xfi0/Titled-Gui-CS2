<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net9.0-windows7.0</TargetFramework>
		<RootNamespace>Titled_Gui</RootNamespace>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<PlatformTarget>x64</PlatformTarget>
		<UseWindowsForms>true</UseWindowsForms>
		<AllowUnsafeBlocks>true</AllowUnsafeBlocks>
		<Platforms>AnyCPU;x64</Platforms>
		<EnableWindowsTargeting>true</EnableWindowsTargeting>
		<ApplicationIcon>Resources\MenuLogo.ico</ApplicationIcon>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Resources\ClickSounds\Creamy.wav" />
	  <None Remove="Resources\fonts\glyph.ttf" />
	  <None Remove="Resources\fonts\Lineicons.ttf" />
	  <None Remove="Resources\fonts\NotoSans-Bold.ttf" />
	  <None Remove="Resources\fonts\undefeated.ttf" />
	  <None Remove="Resources\MenuLogo.png" />
	  <None Remove="Resources\Tutamaka.wav" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="Resources\fonts\glyph.ttf">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Resources\fonts\Lineicons.ttf">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Resources\fonts\NotoSans-Bold.ttf">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Resources\MenuLogo.png">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
		<Content Include="Resources\player.png">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
	  <Content Include="Resources\NeverLose.wav">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Resources\Skeet.wav">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
		<Content Include="Resources\Tutamaka.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\Minecraft.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\tutamaka.png">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\gliter.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\bottle.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\sounds\NeverLose.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
		</Content>
		<Content Include="Resources\sounds\Skeet.wav">
			<CopyToOutputDirectory>Always</CopyToOutputDirectory>
